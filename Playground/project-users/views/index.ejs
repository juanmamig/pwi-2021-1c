<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./partials/meta') %>
</head>
<body>
  <%- include('./partials/header') %>
  <h1>Ecommerce EJS</h1>

  <% products.forEach(product => { %>
    <h3><%= product.name %></h3>
  <% }) %>


  <div class="users-container">
    <% users.forEach(user => { %>
      <div class="user-container">
        <h3><%= user.email %></h3>
      </div>
    <% }) %>
  </div>
</body>
</html>